{
  "name": "html-tailwindcss-boilerplate",
  "version": "1.0.0",
  "description": "a html tailwindcss boilerplate",
  "author": "Jeremy Dayniel David",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production postcss tailwind.css -o src/assets/css/style.css",
    "start": "postcss tailwind.css -o src/assets/css/style.css --watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.14",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.4"
  }
}
